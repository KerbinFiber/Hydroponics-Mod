package kerbinfiber.hydroponicsmod;

import net.minecraft.block.Block;
import net.minecraft.item.Item;
import net.minecraft.item.ItemSeeds;
import net.minecraft.creativetab.CreativeTabs;

public class SeedPacks extends ItemSeeds
{
	
	public SeedPacks(Block HydroTray, Block p_i45352_2_) 
	{
		super(HydroTray, p_i45352_2_);
	}

	public int potency = 5;
	public int yeild = 1;
	public int age = 0;
	
	//Banana Seeds
	
	public class BananaSeeds extends SeedPacks
	{
		
		public BananaSeeds(Block HydroTray, Block p_i45352_2_) 
		{
			super(HydroTray, p_i45352_2_);
			this.setTextureName("hydromod:bananaseedpack");
			this.setUnlocalizedName("BananaSeeds");
			this.setCreativeTab("tabHydroponicsMod");
		}
		
	}

	public class BluespaceBananaSeeds extends SeedPacks
	{

		public BluespaceBananaSeeds(Block HydroTray, Block p_i45352_2_) 
		{
			super(HydroTray, p_i45352_2_);
		}
		
		
		
	}
	
	//Tomato Seeds
	
	public class TomatoSeeds extends SeedPacks
	{

		public TomatoSeeds(Block HydroTray, Block p_i45352_2_) 
		{
			super(HydroTray, p_i45352_2_);
		}
		
		
		
	}

}